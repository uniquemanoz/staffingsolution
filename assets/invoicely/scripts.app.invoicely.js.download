	"use strict";

	var currencyCode = $('[name="statement[currency]"]').val();
	var currencySymbol = currencyCodesIso[(currencyCode.toUpperCase())];
	
	if ($('[name="statement[currency]"]').attr('data-currency-format') === 'code') {
